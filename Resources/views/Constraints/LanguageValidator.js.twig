function Language(value, params)
{
    if (isDefined(value)) {
        return true;
    }

    value = String(value);

    var languages = {{ getLanguages()|raw }};

    for (key in languages) {            
        if (languages[key] == value) {
            return true;
        }
    }
    
    return getComputeMessage(params.message);
}