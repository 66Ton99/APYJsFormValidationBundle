function Country(value, params)
{
    if (isDefined(value)) {
        return true;
    }

    value = String(value);

    var countries = {{ getCountries()|raw }};

    for (key in countries) {            
        if (countries[key] == value) {
            return true;
        }
    }
    
    return getComputeMessage(params.message);
}